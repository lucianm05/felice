{
  "name": "felice",
  "version": "0.0.0",
  "type": "module",
  "description": "Unstyled accessible components",
  "license": "MIT",
  "author": "lucianmg05",
  "keywords": [],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "watch": "vitest",
    "coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@testing-library/dom": "^8.20.0",
    "@testing-library/react": "^13.4.0",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "c8": "^7.12.0",
    "eslint": "^8.34.0",
    "eslint-plugin-react": "^7.32.2",
    "jsdom": "^21.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-test-renderer": "^18.2.0",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vitest": "^0.28.5"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "main": "./dist/my-component.umd.ts",
  "module": "./dist/my-component.es.ts",
  "exports": {
    ".": {
      "import": "./dist/my-component.es.ts",
      "require": "./dist/my-component.umd.ts"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true,
      "es2020": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "parserOptions": {
      "sourceType": "module"
    }
  }
}
